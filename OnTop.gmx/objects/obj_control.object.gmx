            <kind>1</kind>
            <string>///Toggle the window "topmost" status

////////////////////////////// This section converts the window_handle to a standard "real" value //////////////////////////////////////

hex = string_upper(string(window_handle()))
dec = 0
pow = 0

for(i=string_length(hex); i&gt;=1; i-=1) {
  char = string_char_at(hex,i)
  val = string_pos(char,"0123456789ABCDEF") - 1
  dec += val*power(16,pow)
  pow += 1
}

////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

if top {
  ontop_unset(dec)
  background_color = make_color_rgb(198,124,225)
} else {
  ontop_set(dec)
  background_color = make_color_rgb(151,225,124)
}

top = !top
</string>
